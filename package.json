{
  "name": "@miragejs/graphql",
  "version": "0.2.0-alpha.1",
  "description": "A library for handling GraphQL requests with Mirage JS",
  "main": "./dist/mirage-graphql.cjs",
  "type": "module",
  "module": "./dist/mirage-graphql.js",
  "exports": {
    ".": {
      "import": "./dist/mirage-graphql.js",
      "require": "./dist/mirage-graphql.cjs"
    }
  },
  "keywords": [
    "mirage",
    "graphql",
    "fake",
    "mock",
    "server",
    "testing",
    "pretender",
    "prototype"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/miragejs/graphql.git"
  },
  "license": "MIT",
  "author": "Rocky Neurock <jneurock+npm@gmail.com>",
  "bugs": {
    "url": "https://github.com/miragejs/graphql/issues"
  },
  "scripts": {
    "build": "vite build",
    "lint": "eslint .",
    "prepublishOnly": "vite build",
    "prettier:check": "prettier --list-different '**/*.ts'",
    "prettier:update": "prettier --write .",
    "test:generate-gql-types": "graphql-codegen-esm",
    "test:integration": "vitest --config vitest-integration.config.ts --run --coverage",
    "test:unit": "vitest --config vitest-unit.config.ts --run",
    "ts:check": "tsc --skipLibCheck"
  },
  "dependencies": {
    "graphql": "^16.0.0",
    "miragejs": "^0.1.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@rollup/plugin-graphql": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "@vitest/browser": "^0.31.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "graphql-request": "^6.0.0",
    "prettier": "^2.0.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "webdriverio": "^8.10.4"
  },
  "engines": {
    "node": ">= 14.*"
  }
}
